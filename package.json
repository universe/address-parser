{
  "name": "@universe/address-parser",
  "version": "4.0.0",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "description": "Universe address parser.",
  "author": "Adam Miller <adam@universe.app>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "repository": "https://github.com/universe/big-blue",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "bin": {
    "address-parser": "./bin/index.js"
  },
  "scripts": {
    "build": "rm -rf dist && rm -rf esm && tsc -b tsconfig.json",
    "pretest": "yarn run build",
    "test": "mocha dist/test",
