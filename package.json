    "watch": "watch 'yarn run test' './src' './test' --wait=3",
    "prepublish": "yarn run build"
  },
  "dependencies": {
    "@universe/models": "^3.0.10"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/n-readlines": "^1.0.0",
    "fastest-levenshtein": "^1.0.16",
    "mocha": "^10.7.3",
    "n-readlines": "^1.0.0",
    "typescript": "^5.6.2"
  },
  "files": [
    "dist/src/**/*",
    "esm/**/*"
  ]
}
